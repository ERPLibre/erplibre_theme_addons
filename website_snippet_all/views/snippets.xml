<odoo>
    <!-- Snippets -->
    <template id="s_website_snippet_all" name="Website Snippet All">
        <section class="o_website_snippet_all oe_snippet_body">
            <div class="container">
                <div class="row mt16 mb16">
                    <div class="text-center website_snippet_all_value">Hello</div>
                </div>
            </div>
        </section>
    </template>
    <!-- Add snippets to menu -->
    <template id="snippets" inherit_id="website.snippets">
        <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="website_snippet_all.s_website_snippet_all" t-thumbnail="/website_snippet_all/static/description/icon.png"/>
            <t t-snippet="website_snippet_all.s_color_blocks_2" t-thumbnail="/website_snippet_all/static/src/img/snippet/s_color_block.png"/>
            <t t-snippet="website_snippet_all.s_color_blocks_4" t-thumbnail="/website_snippet_all/static/src/img/snippet/s_color_block.png"/>
            <t t-snippet="website_snippet_all.s_column" t-thumbnail="/website_snippet_all/static/src/img/snippet/s_column.png"/>
            <t t-snippet="website_snippet_all.s_two_columns" t-thumbnail="/website_snippet_all/static/src/img/snippet/s_two_columns.png"/>
        </xpath>
    </template>
</odoo>
