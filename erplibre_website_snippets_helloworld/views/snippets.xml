<odoo>
  <!-- Snippet Content-->
  <template id="s_helloworld" name="Helloworld">
    <section>
        <div>
          <h1>Helloworld</h1>
        </div>
    </section>
  </template>

  <!-- Add snippets to menu -->
  <template id="add_snippet_structure_to_menu" inherit_id="website.snippets">
    <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
      <t t-snippet="erplibre_website_snippets_helloworld.s_helloworld" t-thumbnail="/erplibre_website_snippets_helloworld/static/src/img/helloworld.png"/>
    </xpath>
  </template>
</odoo>
